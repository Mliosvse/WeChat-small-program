<view class='starts'>
  <block wx:for="{{arr}}" wx:key="{{index}}">
    <image wx:if="{{item < score.stars/10}}" src="/images/icon/star.png"></image>
    <image wx:if="{{item >= score.stars/10}}" src='/images/icon/none-star.png'></image>
  </block>
  <text class='score'>{{score.average}}</text>
</view>
